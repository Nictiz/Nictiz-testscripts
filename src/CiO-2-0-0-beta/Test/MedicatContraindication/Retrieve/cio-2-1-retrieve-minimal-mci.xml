<TestScript xmlns="http://hl7.org/fhir" xmlns:nts="http://nictiz.nl/xsl/testscript" nts:scenario="client">
    <id value="cio-mci-retrieve-2-1"/>
    <version value="r4-cio-2.0.0-beta"/>
    <name value="CiO 2.0.0-beta - MedicationContraIndication - Retrieve - Scenario 2.1"/>
    <description value="Scenario 2.1 - Retrieve minimally entered medication contraindications"/>
    <nts:authToken patientResourceId="patient-XXX-Zwaan"/>

    <test id="Scenario-2-1">
        <name value="Scenario 2.1"/>
            <description value="Retrieve minimally entered medication contraindications"/>
        <nts:include value="test.client.search" scope="common" nts:in-targets="#default">
            <nts:with-parameter name="description"
                value="Test client to retrieve minimal Flag resources representing CiO building block MedicationContraIndication"/>
            <nts:with-parameter name="resource" value="Flag"/>
            <nts:with-parameter name="params"
                value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999901199&amp;category=http://snomed.info/sct|350241000146102&amp;_include=Flag:patient&amp;_revinclude=Provenance:target"/>
            <nts:with-parameter name="useToken" value="true"/>
        </nts:include>
        <nts:include value="canary-assert.response.successfulSearch" scope="common"/>
        <nts:include value="assert.response.numResources" scope="common"
            resource="Flag" count="1"/>
        <nts:include value="assert.response.numResources" scope="common"
            resource="Patient" count="1"/>
        <nts:include value="assert.response.numResources" scope="common"
            resource="Provenance" count="1"/>
    </test>
