<?xml version="1.0" encoding="UTF-8"?>
<nts:component xmlns="http://hl7.org/fhir" xmlns:nts="http://nictiz.nl/xsl/testscript">
    <nts:parameter name="warningOnly" value="true"/>
    <action>
        <assert>
            <description
                value="Confirm the {$resource} resource contains the comment '{$comment}'."/>
            <direction value="{$direction}"/>
            <expression value="Bundle.entry.resource.as({$resource}).telecom.extension.valueString.exists()"/>
            <warningOnly value="{$warningOnly}"/>
        </assert>
    </action>
</nts:component>