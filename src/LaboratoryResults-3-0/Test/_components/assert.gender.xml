<?xml version="1.0" encoding="UTF-8"?>
<nts:component xmlns="http://hl7.org/fhir" xmlns:nts="http://nictiz.nl/xsl/testscript">
    <nts:parameter name="warningOnly" value="false"/>
    <action>
        <assert>
            <description
                value="Confirm that the Bundle contains a(n) {$resource} with a gender"/>
            <direction value="{$direction}"/>
            <expression value="Bundle.entry.resource.as({$resource}).gender='{$gender}'"/>
            <warningOnly value="{$warningOnly}"/>
        </assert>
    </action>
