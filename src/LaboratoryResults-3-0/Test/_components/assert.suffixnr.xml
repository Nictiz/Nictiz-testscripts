<?xml version="1.0" encoding="UTF-8"?>
<nts:component xmlns="http://hl7.org/fhir" xmlns:nts="http://nictiz.nl/xsl/testscript">
    <nts:parameter name="warningOnly" value="false"/>
    <action>
        <assert>
            <description
                value="Confirm that Bundle contains the right building suffix number for the {$resource}"/>
            <direction value="{$direction}"/>
            <expression value="Bundle.entry.resource.as({$resource}).address.line.extension.where(url='{$urlbuildingsuffixnumber}' and value ='{$suffixnumber}')"/>
            <warningOnly value="{$warningOnly}"/>
        </assert>
    </action>
</nts:component>