<?xml version="1.0" encoding="UTF-8"?>
<TestScript xmlns="http://hl7.org/fhir" xmlns:nts="http://nictiz.nl/xsl/testscript" nts:scenario="server">
   <id value="gbz-fhir3-0-2-ikz-xis-1-1"/>
   <version value="stu3-3.0"/>
   <name value="Nictiz Geboortezorg - Server - Scenario 1.1 - Serve Integrale Zwangerschapskaart resources of current pregnancy of Hanna Kooij"/>
   <description value="Scenario 1.1: Serve Integrale Zwangerschapskaart resources of current pregnancy of Hanna Kooij"/>
   <nts:authToken patientResourceId="HANNAHXXX-KOOIJ"/>
   <variable><name value="dossier-id"/><defaultValue value="0d479b8b-2063-11ec-1751-020000000000"/></variable>
   <variable><name value="T"/><defaultValue value="${CURRENTDATE}"/></variable>
   
   <!-- Patient - Zwangere vrouw -->
   <test id="1-ServeWoman">
      <name value="1-ServeWoman"/>
      <description value="Query Patient resource for HCIM Patient"/>
      <nts:include value="medmij/test.xis.successfulSearch" scope="common" resource="Patient"/>
      <nts:include value="assert.response.numResources" scope="common" nts:in-targets="#default">
         <nts:with-parameter name="resource" value="Patient"/>
         <nts:with-parameter name="count" value="1"/>
         <nts:with-parameter name="atLeast"/>
      </nts:include>
      
      <nts:contentAsserts href="Vrouw1-Kaart1/medmij-gbz-nl-core-patient-HANNAHXXX-KOOIJ.xml" 
         description="has a .birthDate earlier than the other Patient resource expected in the Bundle" 
         selector="iif(Bundle.entry.resource.ofType(Patient)[0].birthDate &lt; Bundle.entry.resource.ofType(Patient)[1].birthDate, Bundle.entry.resource.ofType(Patient)[0], Bundle.entry.resource.ofType(Patient)[1])"
         nts:in-targets="Extended"/>
      <!-- Is het wel de bedoeling dat hier al 2 Patients terugkomen? De count hierboven doet vermoeden van niet -->
      <nts:contentAsserts href="Vrouw1-Kaart2/medmij-gbz-nl-core-patient-HELENXXX-KOOIJ.xml" 
         description="has a .birthDate later than the other Patient resource expected in the Bundle" 
         selector="iif(Bundle.entry.resource.ofType(Patient)[0].birthDate &lt; Bundle.entry.resource.ofType(Patient)[1].birthDate, Bundle.entry.resource.ofType(Patient)[1], Bundle.entry.resource.ofType(Patient)[0])" 
         nts:in-targets="Extended"/>
   </test>
     
   <!-- Actieve zwangerschap (dossier) -->
   <test id="2-ServeMaternalRecord">
      <name value="2-ServeMaternalRecord"/>
      <description value="Test Server query operation for MaternalRecord (EpisodeOfCare) including managing organization (Organization) and care manager (Practitioner)"/>
      <nts:include value="medmij/test.xis.successfulSearch" scope="common">
         <nts:with-parameter name="resource" value="EpisodeOfCare"/>
         <nts:with-parameter name="params" value="?type=http://snomed.info/sct|364320009&amp;status=active&amp;_include=EpisodeOfCare:organization&amp;_include=EpisodeOfCare:care-manager"/>
      </nts:include>
      <nts:include value="assert.response.numResources" scope="common" resource="EpisodeOfCare" count="1" nts:in-targets="#default"/>
      <nts:include value="assert.response.numResources" scope="common" resource="Organization" count="1" nts:in-targets="#default"/>
      <nts:include value="assert.response.numResources" scope="common" resource="Practitioner" count="1" nts:in-targets="#default"/>
      
      <nts:contentAsserts href="Vrouw1-Kaart1/medmij-gbz-bc-MaternalRecord-0d479b8b-2063-11ec-1751-020000000000.xml" 
         nts:in-targets="Extended"/>
      <nts:contentAsserts href="Vrouw1-Kaart1/medmij-gbz-nl-core-organization-2-16-840-1-113883-2-4-6-108001234.xml" 
         nts:in-targets="Extended"/>
      <nts:contentAsserts href="Vrouw1-Kaart1/medmij-gbz-nl-core-practitioner-2-16-528-1-1007-3-112340001.xml" 
         nts:in-targets="Extended"/>
   </test>
  
   <!-- Actieve zwangerschap (zwangerschap) -->
