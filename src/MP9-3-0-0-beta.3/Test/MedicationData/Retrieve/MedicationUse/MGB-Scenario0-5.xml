<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="client">
   <id value="mp9-mgb-Retrieve-0-5"/>
   <version value="r4-mp9-3.0.0-beta"/>
   <name value="Medication Process 9 3.0.0-beta  - MedicationUse - Retrieve - Scenario 0.5"/>
   <description value="Scenario 0.5 - Medicatiegebruik met een filter op gebruiksperiode – einddatum (alles voor T-22)&#xA;(Antwoord: bericht met 3x medicatiegebruik)"/>
   <nts:authToken patientResourceId="nl-core-Patient-mp9-R-vanXXX-Sonnenberg"
                  nts:in-targets="MedMij"/>
   <nts:includeDateT value="yes"/>
   <test id="Scenario-0-5">
      <name value="Scenario 0.5"/>
      <description value="Medicatiegebruik met een filter op gebruiksperiode – einddatum (alles voor T-22)&#xA;(Antwoord: bericht met 3x medicatiegebruik)"/>
      <nts:include value="test.client.search" scope="common" nts:in-targets="#default">
         <nts:with-parameter name="description"
                             value="Test client to retrieve MedicationStatement resource(s) representing MP9 building block MedicationUse"/>
         <nts:with-parameter name="resource" value="MedicationStatement"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999901084&amp;category=http://snomed.info/sct|422979000&amp;period-of-use=le${DATE, T, D,-22}&amp;_include=MedicationStatement:medication"/>
      </nts:include>
      <nts:include value="medmij/test.phr.search"
                   scope="common"
                   nts:in-targets="MedMij">
         <nts:with-parameter name="description"
                             value="Test PHR client to retrieve MedicationStatement resource(s) representing MP9 building block MedicationUse"/>
         <nts:with-parameter name="resource" value="MedicationStatement"/>
         <nts:with-parameter name="params"
                             value="?category=http://snomed.info/sct|422979000&amp;period-of-use=le${DATE, T, D,-22}&amp;_include=MedicationStatement:medication"/>
      </nts:include>
      <nts:include value="canary-assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="MedicationStatement"/>
         <nts:with-parameter name="count" value="3"/>
      </nts:include>
      <nts:include value="assert-returnEntryCountAtLeast" scope="project">
         <nts:with-parameter name="count" value="6"/>
         <nts:with-parameter name="breakdown"
                             value="(Consists of 3 MedicationStatement and 3 Medication resources.)"/>
      </nts:include>
   </test>
</TestScript>
