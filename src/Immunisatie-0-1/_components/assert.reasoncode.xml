<?xml version="1.0" encoding="UTF-8"?>
<nts:component xmlns="http://hl7.org/fhir" xmlns:nts="http://nictiz.nl/xsl/testscript">
    <action>
        <assert>
            <description value="Test if the correct code for route exists" />
            <direction value="{$direction}" /> 
            <expression value= "Bundle.entry.where(resource.is(Immunization)).resource.where(identifier.value = '{$identifier}').where(identifier.system = '{$identifiersystem}').reasonCode.coding.where(system = '{$system}').where(code = '{$code}').exists()" />
        </assert>
    </action>
</nts:component>