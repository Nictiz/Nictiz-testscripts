         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999901084&amp;category=http://snomed.info/sct|373784005&amp;whenhandedover=le${DATE, T, D,-110}&amp;_include=MedicationDispense:medication"/>
      </nts:include>
      <nts:include value="medmij/test.xis.search"
