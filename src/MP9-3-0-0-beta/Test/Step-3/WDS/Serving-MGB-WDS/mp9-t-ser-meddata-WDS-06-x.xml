<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="server">
   <id value="mp9-t-ser-meddata-WDS-06-x"/>
   <version value="r4-mp9-3.0.0"/>
   <name value="mp9-t-ser-meddata-WDS-06-x"/>
   <title value="Scenario 6.x"/>
   <description value="Serve Medication data WDS building blocks for patient MXXX-Rambaldo&#xA;&#xA;For more info:&#xA;https://informatiestandaarden.nictiz.nl/wiki/mp:V9.3.0_testgegevens_medicatiegegevens_beschikbaarstellen_WDS&#xA;&#xA;Original description:&#xA;TEST Medicatiegegevens WDS Scenarioset 6"/>
   <nts:authToken patientResourceId="nl-core-Patient-mp9-MXXX-Rambaldo"
                  nts:in-targets="MedMij"/>
   <nts:includeDateT value="no"/>
   <test id="mp9-t-ser-meddata-WDS-06-x-01">
      <name value="Scenario 6.x"/>
      <description value="Serve Medication data WDS resources in a searchset Bundle"/>
      <nts:include value="test.server.search" scope="common" nts:in-targets="#default">
         <nts:with-parameter name="description"
                             value="Test server to serve MedicationRequest resource(s) representing MP9 building block VariableDosingRegimen"/>
         <nts:with-parameter name="resource" value="MedicationRequest"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999900936&amp;category=http://snomed.info/sct|395067002&amp;_include=MedicationRequest:medication&amp;_include=MedicationRequest:subject"/>
      </nts:include>
      <nts:include value="medmij/test.xis.search"
                   scope="common"
                   nts:in-targets="MedMij">
         <nts:with-parameter name="description"
                             value="Test XIS server to serve MedicationRequest resource(s) representing MP9 building block VariableDosingRegimen"/>
         <nts:with-parameter name="resource" value="MedicationRequest"/>
         <nts:with-parameter name="params"
                             value="?category=http://snomed.info/sct|395067002&amp;_include=MedicationRequest:medication&amp;_include=MedicationRequest:subject"/>
      </nts:include>
      <nts:include value="assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-responseBundleContent-noMM"/>
      <nts:include value="assert-returnCountAtLeast" scope="project">
         <nts:with-parameter name="resource" value="MedicationRequest"/>
         <nts:with-parameter name="count" value="2"/>
      </nts:include>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="Medication"/>
         <nts:with-parameter name="count" value="2"/>
      </nts:include>
   </test>
</TestScript>
