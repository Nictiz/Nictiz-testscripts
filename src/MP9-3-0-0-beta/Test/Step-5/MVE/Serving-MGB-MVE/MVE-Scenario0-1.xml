<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="server">
   <id value="mp9-test-serve-meddata-MVE-00-1"/>
   <version value="r4-mp9-3.0.0-beta"/>
   <title value="Scenario 0.1"/>
   <description value="Serve Medication data MVE building blocks for patient R-vanXXX-Sonnenberg&#xA;&#xA;For more info:&#xA;https://informatiestandaarden.nictiz.nl/wiki/mp:V9.3.0_testgegevens_medicatiegegevens_beschikbaarstellen_MVE&#xA;&#xA;Original description:&#xA;TEST Medicatiegegevens MVE Scenarioset 0 (filterqueries)&#xA;Alle verstrekkingen van de patiÃ«nt, zonder aanvullend filtercriterium&#xA;(Antwoord: bericht met 6 verstrekkingen)"/>
   <nts:authToken patientResourceId="nl-core-Patient-mp9-R-vanXXX-Sonnenberg"
                  nts:in-targets="MedMij"/>
   <test id="mp9-test-serve-meddata-MVE-00-1-01">
      <name value="Scenario 0.1"/>
      <description value="Serve Medication data MVE resources in a searchset Bundle"/>
      <nts:include value="test.server.search" scope="common" nts:in-targets="#default">
         <nts:with-parameter name="description"
                             value="Test server to serve MedicationDispense resource(s) representing MP9 building block MedicationDispense"/>
         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999901084&amp;category=http://snomed.info/sct|373784005&amp;_include=MedicationDispense:medication&amp;_include=MedicationDispense:performer&amp;_include=MedicationDispense:destination&amp;_include:iterate=PractitionerRole:organization&amp;_include:iterate=PractitionerRole:practitioner&amp;_include:iterate=PractitionerRole:location"/>
      </nts:include>
      <nts:include value="medmij/test.xis.search"
                   scope="common"
                   nts:in-targets="MedMij">
         <nts:with-parameter name="description"
                             value="Test XIS server to serve MedicationDispense resource(s) representing MP9 building block MedicationDispense"/>
         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="params"
                             value="?category=http://snomed.info/sct|373784005&amp;_include=MedicationDispense:medication&amp;_include=MedicationDispense:performer&amp;_include=MedicationDispense:destination&amp;_include:iterate=PractitionerRole:organization&amp;_include:iterate=PractitionerRole:practitioner&amp;_include:iterate=PractitionerRole:location"/>
      </nts:include>
      <nts:include value="assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-responseBundleContent-noMM"/>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="count" value="6"/>
      </nts:include>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="Medication"/>
         <nts:with-parameter name="count" value="6"/>
      </nts:include>
   </test>
</TestScript>
