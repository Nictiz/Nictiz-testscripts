<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="server">
   <id value="mp9-mtd-Serve-0-5"/>
   <version value="r4-mp9-3.0.0-beta"/>
   <name value="Medication Process 9 3.0.0-beta  - MedicationAdministration - Serve - Scenario 0.5"/>
   <description value="Scenario 0.5 - Medicatietoedieningen met een filter op gebruiksperiode – einddatum (alles voor T-22)&#xA;(Antwoord: bericht met 3 Medicatietoedieningen)"/>
   <nts:authToken patientResourceId="nl-core-Patient-mp9-R-vanXXX-Sonnenberg"
                  nts:in-targets="MedMij"/>
   <nts:includeDateT value="yes"/>
   <test id="Scenario-0-5">
      <name value="Scenario 0.5"/>
      <description value="Medicatietoedieningen met een filter op gebruiksperiode – einddatum (alles voor T-22)&#xA;(Antwoord: bericht met 3 Medicatietoedieningen)"/>
      <nts:include value="test.server.search" scope="common" nts:in-targets="#default">
         <nts:with-parameter name="description"
                             value="Test server to serve MedicationAdministration resource(s) representing MP9 building block MedicationAdministration"/>
         <nts:with-parameter name="resource" value="MedicationAdministration"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999901084&amp;category=http://snomed.info/sct|18629005&amp;effective-time=le${DATE, T, D,-22}&amp;_include=MedicationAdministration:medication&amp;_include=MedicationAdministration:performer&amp;_include:iterate=PractitionerRole:organization&amp;_include:iterate=PractitionerRole:practitioner&amp;_include:iterate=PractitionerRole:location"/>
      </nts:include>
      <nts:include value="medmij/test.xis.search"
                   scope="common"
                   nts:in-targets="MedMij">
         <nts:with-parameter name="description"
                             value="Test XIS server to serve MedicationAdministration resource(s) representing MP9 building block MedicationAdministration"/>
         <nts:with-parameter name="resource" value="MedicationAdministration"/>
         <nts:with-parameter name="params"
                             value="?category=http://snomed.info/sct|18629005&amp;effective-time=le${DATE, T, D,-22}&amp;_include=MedicationAdministration:medication&amp;_include=MedicationAdministration:performer&amp;_include:iterate=PractitionerRole:organization&amp;_include:iterate=PractitionerRole:practitioner&amp;_include:iterate=PractitionerRole:location"/>
      </nts:include>
      <nts:include value="assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-responseBundleContent-noMM"/>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="MedicationAdministration"/>
         <nts:with-parameter name="count" value="3"/>
      </nts:include>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="Medication"/>
         <nts:with-parameter name="count" value="0"/>
      </nts:include>
   </test>
</TestScript>
