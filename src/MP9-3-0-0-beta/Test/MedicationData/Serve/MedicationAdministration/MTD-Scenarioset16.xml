<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="server">
   <id value="mp9-MedicationAdministration-serve-16-x"/>
   <version value="r4-mp9-3.0.0-beta"/>
   <name value="Medication Process 9 3.0.0-beta  - MedicationAdministration - Serve - Scenario 16.x"/>
   <description value="Scenario 16.x - TEST Medicatiegegevens MTD Scenarioset 16"/>
   <nts:authToken patientResourceId="nl-core-Patient-mp9-JXXX-Valkenet"
                  nts:in-targets="MedMij"/>
   <nts:includeDateT value="no"/>
   <test id="Scenario-16-x">
      <name value="Scenario 16.x"/>
      <description value="TEST Medicatiegegevens MTD Scenarioset 16"/>
      <nts:include value="test.server.search"
                   scope="common"
                   nts:in-targets="#default CheckContent">
         <nts:with-parameter name="description"
                             value="Test server to serve MedicationAdministration resource(s) representing MP9 building block MedicationAdministration"/>
         <nts:with-parameter name="resource" value="MedicationAdministration"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999911089&amp;category=http://snomed.info/sct|18629005&amp;_include=MedicationAdministration:medication&amp;_include=MedicationAdministration:subject&amp;_include=MedicationAdministration:performer&amp;_include:iterate=PractitionerRole:organization&amp;_include:iterate=PractitionerRole:practitioner&amp;_include:iterate=PractitionerRole:location"/>
      </nts:include>
      <nts:include value="medmij/test.xis.search"
                   scope="common"
                   nts:in-targets="MedMij">
         <nts:with-parameter name="description"
                             value="Test XIS server to serve MedicationAdministration resource(s) representing MP9 building block MedicationAdministration"/>
         <nts:with-parameter name="resource" value="MedicationAdministration"/>
         <nts:with-parameter name="params"
                             value="?category=http://snomed.info/sct|18629005&amp;_include=MedicationAdministration:medication&amp;_include=MedicationAdministration:subject&amp;_include=MedicationAdministration:performer&amp;_include:iterate=PractitionerRole:organization&amp;_include:iterate=PractitionerRole:practitioner&amp;_include:iterate=PractitionerRole:location"/>
      </nts:include>
      <nts:include value="assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-responseBundleContent-noMM"/>
      <nts:include value="assert-returnCountAtLeast" scope="project">
         <nts:with-parameter name="resource" value="MedicationAdministration"/>
         <nts:with-parameter name="count" value="5"/>
      </nts:include>
      <nts:include value="assert-returnEntryCountAtLeast" scope="project">
         <nts:with-parameter name="count" value="9"/>
         <nts:with-parameter name="breakdown"
                             value="(Consists of 5 MedicationAdministration and 4 Medication resources.)"/>
      </nts:include>
      <nts:contentAsserts href="fixtures/.xml"
                          description="contains a resource with identifier = MBH_300_redenvanafwijken_MTD"
                          discriminator="identifier.value ='MBH_300_redenvanafwijken_MTD'"
                          nts:in-targets="CheckContent"/>
      <nts:contentAsserts href="fixtures/.xml"
                          description="contains a resource with identifier = MBH_300_adhoctoediening_MTD"
                          discriminator="identifier.value ='MBH_300_adhoctoediening_MTD'"
                          nts:in-targets="CheckContent"/>
      <nts:contentAsserts href="fixtures/.xml"
                          description="contains a resource with identifier = MBH_300_afwijkendeToediener_MTD_Patient"
                          discriminator="identifier.value ='MBH_300_afwijkendeToediener_MTD_Patient'"
                          nts:in-targets="CheckContent"/>
      <nts:contentAsserts href="fixtures/.xml"
                          description="contains a resource with identifier = MBH_300_afwijkendeToediener_MTD_Mantelzorger"
                          discriminator="identifier.value ='MBH_300_afwijkendeToediener_MTD_Mantelzorger'"
                          nts:in-targets="CheckContent"/>
      <nts:contentAsserts href="fixtures/.xml"
                          description="contains a resource with identifier = MBH_300_toedieningRelatieMA_MTD"
                          discriminator="identifier.value ='MBH_300_toedieningRelatieMA_MTD'"
                          nts:in-targets="CheckContent"/>
   </test>
</TestScript>
