<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="client">
   <id value="mp9-vv-Retrieve-0-1"/>
   <version value="r4-mp9-3.0.0-beta"/>
   <name value="Medication Process 9 3.0.0-beta  - DispenseRequest - Retrieve - Scenario 0.1"/>
   <description value="Scenario 0.1 - Alle verstrekkingsverzoeken van de patiënt, zonder aanvullend filtercriterium&#xA;(Antwoord: bericht met 6 verstrekkingsverzoeken)"/>
   <nts:authToken patientResourceId="nl-core-Patient-mp9-R-vanXXX-Sonnenberg"
                  nts:in-targets="MedMij"/>
   <test id="Scenario-0-1">
      <name value="Scenario 0.1"/>
      <description value="Alle verstrekkingsverzoeken van de patiënt, zonder aanvullend filtercriterium&#xA;(Antwoord: bericht met 6 verstrekkingsverzoeken)"/>
      <nts:include value="test.client.search" scope="common" nts:in-targets="#default">
         <nts:with-parameter name="description"
                             value="Test client to retrieve MedicationRequest resource(s) representing MP9 building block DispenseRequest"/>
         <nts:with-parameter name="resource" value="MedicationRequest"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999901084&amp;category=http://snomed.info/sct|52711000146108&amp;_include=MedicationRequest:medication"/>
      </nts:include>
      <nts:include value="medmij/test.phr.search"
                   scope="common"
                   nts:in-targets="MedMij">
         <nts:with-parameter name="description"
                             value="Test PHR client to retrieve MedicationRequest resource(s) representing MP9 building block DispenseRequest"/>
         <nts:with-parameter name="resource" value="MedicationRequest"/>
         <nts:with-parameter name="params"
                             value="?category=http://snomed.info/sct|52711000146108&amp;_include=MedicationRequest:medication"/>
      </nts:include>
      <nts:include value="canary-assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="MedicationRequest"/>
         <nts:with-parameter name="count" value="6"/>
      </nts:include>
      <nts:include value="assert-returnEntryCountAtLeast" scope="project">
         <nts:with-parameter name="count" value="12"/>
         <nts:with-parameter name="breakdown"
                             value="(Consists of 6 MedicationRequest and 6 Medication resources.)"/>
      </nts:include>
   </test>
</TestScript>
