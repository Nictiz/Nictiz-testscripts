<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="client">
   <id value="mp9-Consolidation-AdministrationAgreement-retrieve-12f-x"/>
   <version value="r4-mp9-3.0.0-beta"/>
   <name value="Medication Process 9 3.0.0-beta  - Consolidation - AdministrationAgreement - Retrieve - Scenario 12f.x"/>
   <description value="Scenario 12f.x - TEST Medicatiegegevens Scenarioset 12f (Consolidatie)"/>
   <nts:authToken patientResourceId="nl-core-Patient-mp9-JdeXXX-Groote"
                  nts:in-targets="MedMij"/>
   <nts:includeDateT value="no"/>
   <test id="Scenario-12f-x">
      <name value="Scenario 12f.x"/>
      <description value="TEST Medicatiegegevens Scenarioset 12f (Consolidatie)"/>
      <nts:include value="test.client.search" scope="common" nts:in-targets="#default">
         <nts:with-parameter name="description"
                             value="Test client to retrieve MedicationDispense resource(s) representing MP9 building block AdministrationAgreement"/>
         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999909265&amp;category=http://snomed.info/sct|422037009&amp;_include=MedicationDispense:medication&amp;_include=MedicationDispense:subject"/>
      </nts:include>
      <nts:include value="medmij/test.phr.search"
                   scope="common"
                   nts:in-targets="MedMij">
         <nts:with-parameter name="description"
                             value="Test PHR client to retrieve MedicationDispense resource(s) representing MP9 building block AdministrationAgreement"/>
         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="params"
                             value="?category=http://snomed.info/sct|422037009&amp;_include=MedicationDispense:medication&amp;_include=MedicationDispense:subject"/>
      </nts:include>
      <nts:include value="canary-assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="count" value="1"/>
      </nts:include>
      <action>
         <assert>
            <description value="Returned Medication count is 0"/>
            <direction value="response"/>
            <expression value="Bundle.entry.resource.where($this is Medication).count() = 0"/>
            <stopTestOnFail value="false"/>
            <warningOnly value="false"/>
         </assert>
      </action>
   </test>
</TestScript>
