<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="client">
   <id value="mp9-ma-Retrieve-0-6"/>
   <version value="r4-mp9-3.0.0-beta"/>
   <name value="Medication Process 9 3.0.0-beta  - MedicationAgreement - Retrieve - Scenario 0.6"/>
   <description value="Scenario 0.6 - Medicatieafspraken met een filter op gebruiksperiode – ingangsdatum én einddatum (alles vanaf T-21 tot en met T-7)&#xA;(Antwoord: bericht met 3 medicatieafspraken)"/>
   <nts:authToken patientResourceId="nl-core-Patient-mp9-R-vanXXX-Sonnenberg"
                  nts:in-targets="MedMij"/>
   <nts:includeDateT value="yes"/>
   <test id="Scenario-0-6">
      <name value="Scenario 0.6"/>
      <description value="Medicatieafspraken met een filter op gebruiksperiode – ingangsdatum én einddatum (alles vanaf T-21 tot en met T-7)&#xA;(Antwoord: bericht met 3 medicatieafspraken)"/>
      <nts:include value="test.client.search" scope="common" nts:in-targets="#default">
         <nts:with-parameter name="description"
                             value="Test client to retrieve MedicationRequest resource(s) representing MP9 building block MedicationAgreement"/>
         <nts:with-parameter name="resource" value="MedicationRequest"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999901084&amp;category=http://snomed.info/sct|33633005&amp;period-of-use=ge${DATE, T, D,-21}&amp;period-of-use=le${DATE, T, D,-7}&amp;_include=MedicationRequest:medication"/>
      </nts:include>
      <nts:include value="medmij/test.phr.search"
                   scope="common"
                   nts:in-targets="MedMij">
         <nts:with-parameter name="description"
                             value="Test PHR client to retrieve MedicationRequest resource(s) representing MP9 building block MedicationAgreement"/>
         <nts:with-parameter name="resource" value="MedicationRequest"/>
         <nts:with-parameter name="params"
                             value="?category=http://snomed.info/sct|33633005&amp;period-of-use=ge${DATE, T, D,-21}&amp;period-of-use=le${DATE, T, D,-7}&amp;_include=MedicationRequest:medication"/>
      </nts:include>
      <nts:include value="canary-assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="MedicationRequest"/>
         <nts:with-parameter name="count" value="3"/>
      </nts:include>
      <nts:include value="assert-returnEntryCountAtLeast" scope="project">
         <nts:with-parameter name="count" value="6"/>
         <nts:with-parameter name="breakdown"
                             value="(Consists of 3 MedicationRequest and 3 Medication resources.)"/>
      </nts:include>
   </test>
</TestScript>
