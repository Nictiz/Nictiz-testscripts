<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="client">
   <id value="mp9-Consolidation-AdministrationAgreement-retrieve-12e-x"/>
