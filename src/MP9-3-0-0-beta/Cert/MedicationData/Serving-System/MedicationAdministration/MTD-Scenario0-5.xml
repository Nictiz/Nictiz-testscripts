<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="server">
   <id value="mp9-mtd-Serve-0-5"/>
   <version value="r4-mp9-3.0.0-beta"/>
   <name value="Medication Process 9 3.0.0-beta  - MedicationAdministration - Serve - Scenario 0.5"/>
   <description value="Scenario 0.5 - Medicatietoedieningen met een filter op gebruiksperiode – einddatum (alles voor T-21)&#xA;(Antwoord: bericht met 5x medicatietoedieningen)"/>
   <nts:authToken patientResourceId="nl-core-Patient-mp9-G-XXX-Groot"
                  nts:in-targets="MedMij"/>
   <nts:includeDateT value="yes"/>
   <test id="Scenario-0-5">
      <name value="Scenario 0.5"/>
      <description value="Medicatietoedieningen met een filter op gebruiksperiode – einddatum (alles voor T-21)&#xA;(Antwoord: bericht met 5x medicatietoedieningen)"/>
      <nts:include value="test.server.search" scope="common" nts:in-targets="#default">
         <nts:with-parameter name="description"
                             value="Test server to serve MedicationAdministration resource(s) representing MP9 building block MedicationAdministration"/>
         <nts:with-parameter name="resource" value="MedicationAdministration"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999900596&amp;category=http://snomed.info/sct|18629005&amp;effective-time=le${DATE, T, D,-21}&amp;_include=MedicationAdministration:medication&amp;_include=MedicationAdministration:performer&amp;_include:iterate=PractitionerRole:organization&amp;_include:iterate=PractitionerRole:practitioner&amp;_include:iterate=PractitionerRole:location"/>
      </nts:include>
      <nts:include value="medmij/test.xis.search"
                   scope="common"
                   nts:in-targets="MedMij">
         <nts:with-parameter name="description"
                             value="Test XIS server to serve MedicationAdministration resource(s) representing MP9 building block MedicationAdministration"/>
         <nts:with-parameter name="resource" value="MedicationAdministration"/>
         <nts:with-parameter name="params"
                             value="?category=http://snomed.info/sct|18629005&amp;effective-time=le${DATE, T, D,-21}&amp;_include=MedicationAdministration:medication&amp;_include=MedicationAdministration:performer&amp;_include:iterate=PractitionerRole:organization&amp;_include:iterate=PractitionerRole:practitioner&amp;_include:iterate=PractitionerRole:location"/>
      </nts:include>
      <nts:include value="assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-responseBundleContent-noMM"/>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="MedicationAdministration"/>
         <nts:with-parameter name="count" value="5"/>
      </nts:include>
      <nts:include value="assert-returnEntryCountAtLeast" scope="project">
         <nts:with-parameter name="count" value="6"/>
         <nts:with-parameter name="breakdown"
                             value="(Consists of 5 MedicationAdministration and 1 Medication resources.)"/>
      </nts:include>
      <action>
         <assert>
            <description value="All returned MedicationAdministration identifiers are from the expected allow-list."/>
            <direction value="response"/>
            <expression value="Bundle.entry.resource.where($this is MedicationAdministration).identifier.value.all($this in {'MBH_300_script0_QA3_MTD1', 'MBH_300_script0_QA3_MTD2', 'MBH_300_script0_QA3_MTD3', 'MBH_300_script0_QA3_MTD4', 'MBH_300_script0_QA3_MTD5'})"/>
            <stopTestOnFail value="false"/>
            <warningOnly value="false"/>
         </assert>
      </action>
      <action>
         <assert>
            <description value="Returned MedicationAdministration count is at least 5."/>
            <direction value="response"/>
            <expression value="Bundle.entry.resource.where($this is MedicationAdministration).count() &gt;= 5"/>
            <stopTestOnFail value="false"/>
            <warningOnly value="false"/>
         </assert>
      </action>
      <action>
         <assert>
            <description value="Returned MedicationAdministration count is at most 5."/>
            <direction value="response"/>
            <expression value="Bundle.entry.resource.where($this is MedicationAdministration).count() &lt;= 5"/>
            <stopTestOnFail value="false"/>
            <warningOnly value="false"/>
         </assert>
      </action>
      <action>
         <assert>
            <description value="Exact identifier set match for MedicationAdministration (no period-of-use filter)."/>
            <direction value="response"/>
            <expression value="Bundle.entry.resource.where($this is MedicationAdministration).identifier.value.where($this in {'MBH_300_script0_QA3_MTD1', 'MBH_300_script0_QA3_MTD2', 'MBH_300_script0_QA3_MTD3', 'MBH_300_script0_QA3_MTD4', 'MBH_300_script0_QA3_MTD5'}).count() = 5"/>
            <stopTestOnFail value="false"/>
            <warningOnly value="false"/>
         </assert>
      </action>
   </test>
</TestScript>
