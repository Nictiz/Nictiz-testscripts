<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="client">
   <id value="mp9-ta-Retrieve-0-1"/>
   <version value="r4-mp9-3.0.0-beta"/>
   <name value="Medication Process 9 3.0.0-beta  - AdministrationAgreement - Retrieve - Scenario 0.1"/>
   <description value="Scenario 0.1 - &lt;div&gt;Alle toedieningsafspraken van de patiënt, zonder aanvullend filtercriterium (Antwoord: bericht met 5 toedieningsafspraken)&lt;/div&gt;"/>
   <nts:authToken patientResourceId="nl-core-Patient-mp9-G-XXX-Groot"
                  nts:in-targets="MedMij"/>
   <test id="Scenario-0-1">
      <name value="Scenario 0.1"/>
      <description value="&lt;div&gt;Alle toedieningsafspraken van de patiënt, zonder aanvullend filtercriterium (Antwoord: bericht met 5 toedieningsafspraken)&lt;/div&gt;"/>
      <nts:include value="test.client.search" scope="common" nts:in-targets="#default">
         <nts:with-parameter name="description"
                             value="Test client to retrieve MedicationDispense resource(s) representing MP9 building block AdministrationAgreement"/>
         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999900596&amp;category=http://snomed.info/sct|422037009&amp;_include=MedicationDispense:medication"/>
      </nts:include>
      <nts:include value="medmij/test.phr.search"
                   scope="common"
                   nts:in-targets="MedMij">
         <nts:with-parameter name="description"
                             value="Test PHR client to retrieve MedicationDispense resource(s) representing MP9 building block AdministrationAgreement"/>
         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="params"
                             value="?category=http://snomed.info/sct|422037009&amp;_include=MedicationDispense:medication"/>
      </nts:include>
      <nts:include value="canary-assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="MedicationDispense"/>
         <nts:with-parameter name="count" value="5"/>
      </nts:include>
      <nts:include value="assert-returnEntryCountAtLeast" scope="project">
         <nts:with-parameter name="count" value="9"/>
         <nts:with-parameter name="breakdown"
                             value="(Consists of 5 MedicationDispense and 4 Medication resources.)"/>
      </nts:include>
   </test>
</TestScript>
