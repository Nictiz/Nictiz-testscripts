<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="server">
   <id value="mp9-VariableDosingRegimen-serve-10-x"/>
   <version value="r4-mp9-3.0.0-beta"/>
   <name value="Medication Process 9 3.0.0-beta  - VariableDosingRegimen - Serve - Scenario 10.x"/>
   <description value="Scenario 10.x - KWAL Medicatiegegevens WDS Script 10 (filterqueries pre-livegang kwalificatie)"/>
   <nts:authToken patientResourceId="nl-core-Patient-mp9-CXXX-Perfini"
                  nts:in-targets="MedMij"/>
   <nts:includeDateT value="no"/>
   <test id="Scenario-10-x">
      <name value="Scenario 10.x"/>
      <description value="KWAL Medicatiegegevens WDS Script 10 (filterqueries pre-livegang kwalificatie)"/>
      <nts:include value="test.server.search"
                   scope="common"
                   nts:in-targets="#default CheckContent">
         <nts:with-parameter name="description"
                             value="Test server to serve MedicationRequest resource(s) representing MP9 building block VariableDosingRegimen"/>
         <nts:with-parameter name="resource" value="MedicationRequest"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999908911&amp;category=http://snomed.info/sct|395067002&amp;_include=MedicationRequest:medication&amp;_include=MedicationRequest:subject"/>
      </nts:include>
      <nts:include value="medmij/test.xis.search"
                   scope="common"
                   nts:in-targets="MedMij">
         <nts:with-parameter name="description"
                             value="Test XIS server to serve MedicationRequest resource(s) representing MP9 building block VariableDosingRegimen"/>
         <nts:with-parameter name="resource" value="MedicationRequest"/>
         <nts:with-parameter name="params"
                             value="?category=http://snomed.info/sct|395067002&amp;_include=MedicationRequest:medication&amp;_include=MedicationRequest:subject"/>
      </nts:include>
      <nts:include value="assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-responseBundleContent-noMM"/>
      <nts:include value="assert-returnCountAtLeast" scope="project">
         <nts:with-parameter name="resource" value="MedicationRequest"/>
         <nts:with-parameter name="count" value="4"/>
      </nts:include>
      <nts:include value="assert-returnEntryCountAtLeast" scope="project">
         <nts:with-parameter name="count" value="6"/>
         <nts:with-parameter name="breakdown"
                             value="(Consists of 4 MedicationRequest and 2 Medication resources.)"/>
      </nts:include>
      <nts:contentAsserts href="fixtures/mp-VarDosReg-mp9-MBH300script10QA3WDS1.xml"
                          description="contains a resource with identifier = MBH_300_script10_QA3_WDS1"
                          discriminator="identifier.value ='MBH_300_script10_QA3_WDS1'"
                          nts:in-targets="CheckContent"/>
      <nts:contentAsserts href="fixtures/mp-VarDosReg-mp9-MBH300script10QA3WDS2.xml"
                          description="contains a resource with identifier = MBH_300_script10_QA3_WDS2"
                          discriminator="identifier.value ='MBH_300_script10_QA3_WDS2'"
                          nts:in-targets="CheckContent"/>
      <nts:contentAsserts href="fixtures/mp-VarDosReg-mp9-MBH300script0QA3WDS1.xml"
                          description="contains a resource with identifier = MBH_300_script0_QA3_WDS1"
                          discriminator="identifier.value ='MBH_300_script0_QA3_WDS1'"
                          nts:in-targets="CheckContent"/>
      <nts:contentAsserts href="fixtures/mp-VarDosReg-mp9-MBH300script0QA3WDS2.xml"
                          description="contains a resource with identifier = MBH_300_script0_QA3_WDS2"
                          discriminator="identifier.value ='MBH_300_script0_QA3_WDS2'"
                          nts:in-targets="CheckContent"/>
   </test>
</TestScript>
