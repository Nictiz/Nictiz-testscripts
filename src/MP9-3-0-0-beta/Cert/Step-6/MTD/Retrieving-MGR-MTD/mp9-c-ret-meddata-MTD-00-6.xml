<TestScript xmlns:nts="http://nictiz.nl/xsl/testscript"
            xmlns="http://hl7.org/fhir"
            nts:scenario="client">
   <id value="mp9-c-ret-meddata-MTD-00-6"/>
   <version value="r4-mp9-3.0.0"/>
   <name value="mp9-c-ret-meddata-MTD-00-6"/>
   <title value="Scenario 0.6"/>
   <description value="Retrieve Medication data MTD building blocks for patient G-XXX-Groot&#xA;&#xA;For more info:&#xA;https://informatiestandaarden.nictiz.nl/wiki/mp:V9.3.0_kwalificatie_medicatiegegevens_raadplegen_MTD&#xA;&#xA;Original description:&#xA;KWAL Medicatiegegevens MTD script0 (filterqueries)&#xA;Medicatietoedieningen met een filter op gebruiksperiode – ingangsdatum én einddatum (alles vanaf T-23 tot en met T-11)&#xA;(Antwoord: bericht met 7x medicatietoedieningen) "/>
   <nts:authToken patientResourceId="nl-core-Patient-mp9-G-XXX-Groot"
                  nts:in-targets="MedMij"/>
   <nts:includeDateT value="yes"/>
   <test id="mp9-c-ret-meddata-MTD-00-6-01">
      <name value="Scenario 0.6"/>
      <description value="Retrieve Medication data MTD resources in a searchset Bundle"/>
      <nts:include value="test.client.search" scope="common" nts:in-targets="#default">
         <nts:with-parameter name="description"
                             value="Test client to retrieve MedicationAdministration resource(s) representing MP9 building block MedicationAdministration"/>
         <nts:with-parameter name="resource" value="MedicationAdministration"/>
         <nts:with-parameter name="params"
                             value="?patient.identifier=http://fhir.nl/fhir/NamingSystem/bsn|999900596&amp;category=http://snomed.info/sct|18629005&amp;effective-time=ge${DATE, T, D,-23}&amp;effective-time=le${DATE, T, D,-11}&amp;_include=MedicationAdministration:medication&amp;_include=MedicationAdministration:performer&amp;_include:iterate=PractitionerRole:organization&amp;_include:iterate=PractitionerRole:practitioner&amp;_include:iterate=PractitionerRole:location"/>
      </nts:include>
      <nts:include value="medmij/test.phr.search"
                   scope="common"
                   nts:in-targets="MedMij">
         <nts:with-parameter name="description"
                             value="Test PHR client to retrieve MedicationAdministration resource(s) representing MP9 building block MedicationAdministration"/>
         <nts:with-parameter name="resource" value="MedicationAdministration"/>
         <nts:with-parameter name="params"
                             value="?category=http://snomed.info/sct|18629005&amp;effective-time=ge${DATE, T, D,-23}&amp;effective-time=le${DATE, T, D,-11}&amp;_include=MedicationAdministration:medication&amp;_include=MedicationAdministration:performer&amp;_include:iterate=PractitionerRole:organization&amp;_include:iterate=PractitionerRole:practitioner&amp;_include:iterate=PractitionerRole:location"/>
      </nts:include>
      <nts:include value="canary-assert.response.successfulSearch" scope="common"/>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="MedicationAdministration"/>
         <nts:with-parameter name="count" value="7"/>
      </nts:include>
      <nts:include value="assert-returnCount" scope="project">
         <nts:with-parameter name="resource" value="Medication"/>
         <nts:with-parameter name="count" value="3"/>
      </nts:include>
   </test>
</TestScript>
