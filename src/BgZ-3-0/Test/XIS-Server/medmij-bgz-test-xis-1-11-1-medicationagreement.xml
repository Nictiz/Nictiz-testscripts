<?xml version="1.0" encoding="UTF-8"?><TestScript xmlns="http://hl7.org/fhir" xmlns:nts="http://nictiz.nl/xsl/testscript" nts:scenario="server">
   <id value="medmij-bgz-test-xis-1-11-1"/>
   <version value="stu3-3.0"/>
   <name value="BgZ - XIS Server - Test - Scenario 1.11.1 - Serve BgZ MedicationAgreement resources of XXX_Rijn"/>
   <description value="Scenario 1.11.1 - Serve BgZ MedicationAgreement resources of XXX_Rijn"/>
   <nts:authToken patientResourceId="medmij-bgz-test-patA"/>
   <test>
      <name value="Scenario 1.11.1 - MedicationAgreement"/>
      <description value="Serve MedicationRequest resource(s) for zib MedicationAgreement and include Medication"/>
      <nts:include value="medmij/test.xis.successfulSearch" scope="common" resource="MedicationRequest" params="?category=http://snomed.info/sct|16076005&amp;_include=MedicationRequest:medication"/>
      <nts:include value="assert.response.numResources" scope="common" resource="MedicationRequest" count="2"/>
      <nts:include value="assert.response.numResources" scope="common" resource="Medication" count="2"/>
      
      <nts:contentAsserts href="resources/zib-MedicationAgreement-medmij-bgz-test-patA-medagr1.xml" description="contains .modifierExtension('http://nictiz.nl/fhir/StructureDefinition/zib-Medication-StopType') with .valueCodeableConcept.coding.code = '1'" expression=".where(modifierExtension.where(url = 'http://nictiz.nl/fhir/StructureDefinition/zib-Medication-StopType').value.ofType(CodeableConcept).where(coding.where(code = '1')))"/>
      <nts:contentAsserts href="resources/zib-MedicationAgreement-medmij-bgz-test-patA-medagr2.xml" description="contains .modifierExtension('http://nictiz.nl/fhir/StructureDefinition/zib-Medication-StopType') with .valueCodeableConcept.coding.code = '2'" expression=".where(modifierExtension.where(url = 'http://nictiz.nl/fhir/StructureDefinition/zib-Medication-StopType').value.ofType(CodeableConcept).where(coding.where(code = '2')))"/>
      
      <nts:contentAsserts href="resources/zib-Product-medmij-bgz-test-patA-product1.xml" description="contains .code.coding.code = '2182416'" expression=".where(code.where(coding.where(code = '2182416')))"/>
      <nts:contentAsserts href="resources/zib-Product-medmij-bgz-test-patA-product2.xml" description="contains .code.coding.code = '2613093'" expression=".where(code.where(coding.where(code = '2613093')))"/>
   </test>
</TestScript>