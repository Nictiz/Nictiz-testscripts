<?xml version="1.0" encoding="UTF-8"?><TestScript xmlns="http://hl7.org/fhir" xmlns:nts="http://nictiz.nl/xsl/testscript" nts:scenario="server">
   <id value="medmij-bgz-test-xis-1-13"/>
   <version value="stu3-3.0"/>
   <name value="BgZ - XIS Server - Test - Scenario 1.13 - Retrieve BgZ Vaccination resources of XXX_Rijn"/>
   <description value="Scenario 1.13 - Serve BgZ Vaccination resources of XXX_Rijn"/>
   <nts:authToken patientResourceId="medmij-bgz-test-patA"/>
   <test>
      <name value="Scenario 1.13 - Vaccination"/>
      <description value="Serve Immunization resource(s) for zib Vaccination"/>
      <nts:include value="medmij/test.xis.successfulSearch" scope="common" resource="Immunization" params="?status=completed"/>
      
      <nts:contentAsserts href="resources/zib-Vaccination-medmij-bgz-test-patA-vaccin1.xml" description="has a .date earlier than the other Immunization resource" expression="iif(Bundle.entry.resource.ofType(Immunization)[0].date &lt; Bundle.entry.resource.ofType(Immunization)[1].date, Bundle.entry.resource.ofType(Immunization)[0], Bundle.entry.resource.ofType(Immunization)[1])" bundleExpression="true"/>
      <nts:contentAsserts href="resources/zib-Vaccination-medmij-bgz-test-patA-vaccin2.xml" description="has a .date later than the other Immunization resource" expression="iif(Bundle.entry.resource.ofType(Immunization)[0].date &lt; Bundle.entry.resource.ofType(Immunization)[1].date, Bundle.entry.resource.ofType(Immunization)[1], Bundle.entry.resource.ofType(Immunization)[0])" bundleExpression="true"/>
   </test>
</TestScript>