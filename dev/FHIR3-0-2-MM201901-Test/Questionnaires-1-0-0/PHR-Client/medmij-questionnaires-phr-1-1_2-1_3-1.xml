         <reference value="../_reference/resources/resources-specific/medmij-questionnaires-vl-QuestionnaireProvisioningTask-XXX_Vink-Intake-ACCEPTED.xml"/>
      </resource>
   </fixture>
   <fixture id="task-minimum-fixture-xml">
      <resource>
         <reference value="../_reference/resources/resources-specific/medmij-questionnaires-vl-QuestionnaireProvisioningTask-XXX_Vink-Intake-minimum.xml"/>
