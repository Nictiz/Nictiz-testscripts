   <version value="stu3-1.0.0"/>
   <name value="MedMij Questionnaires - PHR Client - Scenario 1.1, 2.1, 3.1"/>
   <status value="active"/>
   <date value="2021-02-11"/>
   <publisher value="Nictiz"/>
   <contact>
      <name value="Nictiz"/>
      <telecom>
         <system value="email"/>
         <value value="kwalificatie@nictiz.nl"/>
