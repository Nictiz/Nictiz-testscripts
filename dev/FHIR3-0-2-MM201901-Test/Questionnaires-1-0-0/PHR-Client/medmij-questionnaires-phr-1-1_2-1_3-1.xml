      </profile>
   </destination>
   <fixture id="patient-token-fixture">
      <resource>
         <reference value="../_reference/medmij-questionnaires-nl-core-patient-XXX_Vink-token.xml"/>
      </resource>
   </fixture>
   <fixture id="task-requested-fixture">
