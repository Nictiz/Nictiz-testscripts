            <direction value="response"/>
            <expression value="Bundle.entry.resource.ofType(NutritionOrder).where(id = '${zib-NutritionAdvice-medmij-bgz-test-patA-nutrition1-id}').oralDiet.texture.where(modifier.text.replaceMatches('[ .,_-]+', ' ') ~ 'Vast' and foodType.text.replaceMatches('[ .,_-]+', ' ') ~ 'Vast').exists()"/>
