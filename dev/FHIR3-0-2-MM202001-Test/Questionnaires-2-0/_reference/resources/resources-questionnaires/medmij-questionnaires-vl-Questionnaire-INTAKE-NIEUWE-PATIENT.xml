      <type value="choice"/>
      <enableWhen>
        <question value="roken"/>
        <answerBoolean value="true"/>
      </enableWhen>
      <option>
        <valueCoding>
          <code value="DAG"/>
          <display value="Dagelijks"/>
        </valueCoding>
      </option>
      <option>
        <valueCoding>
          <code value="BDA"/>
          <display value="Bijna dagelijks"/>
        </valueCoding>
      </option>
      <option>
        <valueCoding>
          <code value="PDW"/>
          <display value="Een paar dagen per week"/>
        </valueCoding>
      </option>
      <option>
        <valueCoding>
          <code value="WKD"/>
          <display value="Alleen in het weekend"/>
        </valueCoding>
      </option>
      <option>
        <valueCoding>
          <code value="AFT"/>
          <display value="Af en toe"/>
        </valueCoding>
      </option>
      <option>
        <valueCoding>
          <code value="ZEL"/>
          <display value="Zelden"/>
        </valueCoding>
      </option>
    </item>
    <item>
      <linkId value="drinken"/>
      <text value="Drinkt u alcohol?"/>
      <type value="boolean"/>
    </item>
    <item>
      <linkId value="hoevaakdrinken"/>
      <text value="Hoe vaak per week drinkt u alcohol?"/>
      <type value="quantity"/>
      <enableWhen>
        <question value="drinken"/>
        <answerBoolean value="true"/>
      </enableWhen>
    </item>
    <item>
      <linkId value="lichamelijkegezondheid"/>
