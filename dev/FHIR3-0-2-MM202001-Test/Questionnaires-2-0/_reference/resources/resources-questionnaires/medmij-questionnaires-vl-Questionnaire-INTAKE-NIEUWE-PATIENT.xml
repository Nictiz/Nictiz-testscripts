      <enableWhen>
        <question value="sport"/>
        <answerBoolean value="true"/>
      </enableWhen>
      <repeats value="true"/>
      <option>
        <valueCoding>
          <code value="VOE"/>
          <display value="Voetbal"/>
        </valueCoding>
      </option>
      <option>
        <valueCoding>
          <code value="TEN"/>
          <display value="Tennis"/>
        </valueCoding>
      </option>
      <option>
        <valueCoding>
          <code value="HOC"/>
          <display value="Hockey"/>
        </valueCoding>
      </option>
      <option>
        <valueCoding>
          <code value="VOL"/>
          <display value="Volleybal"/>
        </valueCoding>
      </option>
      <option>
        <valueCoding>
          <code value="BAS"/>
          <display value="Basketbal"/>
        </valueCoding>
      </option>
      <option>
        <valueCoding>
          <code value="ZWE"/>
          <display value="Zwemmen"/>
        </valueCoding>
      </option>
      <option>
        <valueCoding>
          <code value="YOG"/>
          <display value="Yoga"/>
        </valueCoding>
      </option>
      <option>
        <valueCoding>
          <code value="AND"/>
          <display value="Anders, namelijk"/>
        </valueCoding>
      </option>
    </item>
    <item>
      <linkId value="welkeanderesport"/>
      <text value="Anders, namelijk"/>
      <type value="string"/>
      <enableWhen>
        <question value="welkesport"/>
        <answerCoding>
          <code value="AND"/>
          <display value="Anders, namelijk"/>
        </answerCoding>
      </enableWhen>
      <repeats value="true"/>
    </item>
  </item>
