  <item>
    <linkId value="introductie"/>
    <text value="Met het beantwoorden van deze vragenlijst voorziet u ons van enkele basisgegevens die we opnemen in uw dossier."/>
    <type value="display"/>
  </item>
  <item>
    <linkId value="algemeen"/>
    <text value="Algemene vragen"/>
    <type value="group"/>
    <item>
      <extension url="http://hl7.org/fhir/StructureDefinition/questionnaire-unit">
        <valueCoding>
          <system value="http://unitsofmeasure.org"/>
          <code value="kg"/>
        </valueCoding>
      </extension>
      <extension url="http://hl7.org/fhir/StructureDefinition/maxDecimalPlaces">
        <valueInteger value="1"/>
      </extension>
      <linkId value="gewicht"/>
      <text value="Wat is uw gewicht in kg?"/>
      <type value="decimal"/>
      <required value="true"/>
    </item>
    <item>
      <extension url="http://hl7.org/fhir/StructureDefinition/questionnaire-unit">
        <valueCoding>
          <system value="http://unitsofmeasure.org"/>
          <code value="cm"/>
        </valueCoding>
      </extension>
      <extension url="http://hl7.org/fhir/StructureDefinition/minLength">
        <valueInteger value="0"/>
      </extension>
      <linkId value="lengte"/>
      <text value="Wat is uw lengte in cm?"/>
      <type value="integer"/>
      <required value="true"/>
    </item>
    <item>
      <linkId value="roken"/>
      <text value="Rookt u?"/>
      <type value="boolean"/>
    </item>
    <item>
      <linkId value="ooitgerookt"/>
      <text value="U rookt niet meer, maar heeft u ooit gerookt?"/>
      <type value="boolean"/>
      <enableWhen>
        <question value="roken"/>
        <answerBoolean value="false"/>
      </enableWhen>
    </item>
    <item>
      <extension url="http://hl7.org/fhir/StructureDefinition/questionnaire-unit">
        <valueCoding>
          <system value="http://unitsofmeasure.org"/>
          <code value="a"/>
        </valueCoding>
      </extension>
      <linkId value="jarensindsroken"/>
      <text value="Hoe lang is het geleden dat u heeft gerookt (in jaren)?"/>
